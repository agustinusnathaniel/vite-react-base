{
  "name": "vite-react-ts",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "pnpm biome lint ./src",
    "toolchain:fix": "pnpm biome check --apply ./src vite.config.ts commitlint.config.cjs",
    "type-check": "tsc -b",
    "turbo": "pnpm dlx turbo@2.0.11",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage run",
    "check:turbo": "pnpm turbo lint type-check test",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.0.20",
    "million": "3.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "typescript": "5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-pwa": "^0.20.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
}
