{
  "name": "vite-react-ts",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "toolchain:fix": "pnpm dlx @biomejs/biome check --apply ./src vite.config.ts commitlint.config.cjs",
    "type-check": "tsc",
    "check:turbo": "pnpm dlx turbo@latest lint type-check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.0.18",
    "million": "2.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/node": "^20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "typescript": "5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-pwa": "^0.17.4",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  }
}
